package moupro;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class FirstAutoClass {

	public static void main(String[] args) throws InterruptedException {
		System.setProperty("webdriver.chrome.driver", "C:\\driver\\chromedriver.exe");
         
		
		String testemail= "mousa.manager";
		String testPass = "0000";
       WebDriver driver = new ChromeDriver();
       
       driver.get("https://web.stage.agentsoncloud.com");
       
       driver.manage().window().maximize();
       
       driver.findElement(By.id("input-8658")).sendKeys(testemail);
       driver.findElement(By.id("input-8661")).sendKeys(testPass);
       driver.findElement(By.xpath("//*[@id=\"app\"]/div/div/div/div[2]/div/span/div/div/div/button/span")).click();
       
      // driver.findElement(By.xpath("//a[normalize-space()='Forgot Password?']")).click();

	}
       
}
